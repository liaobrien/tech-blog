
<h2 class="text-dark m-5">Dashboard</h2>
<a href="/dashboard/new-post">
      <button class="btn btn-dark mx-5">+ New Post</button>
</a>
{{#each posts as |post|}}
<a href="/dashboard/edit/{{post.id}}"><div class="card m-5">
      <div class="card-header">
            <h3 class="text-dark">{{post.title}}</h3>
            <h4 class="text-dark">{{formatDate post_date}}</h4>
      </div>
</div></a>
{{/each}}
